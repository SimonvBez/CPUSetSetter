<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="BackgroundBrush" Color="#1E1E1E"/>
    <SolidColorBrush x:Key="ControlBackgroundBrush" Color="#292929"/>
    <SolidColorBrush x:Key="DisabledBackgroundBrush" Color="#252526"/>
    <SolidColorBrush x:Key="ClickableBackgroundBrush" Color="#202020"/>
    <SolidColorBrush x:Key="HoverBackgroundBrush" Color="#213038"/>
    <SolidColorBrush x:Key="SelectedBackgroundBrush" Color="#094771"/>

    <SolidColorBrush x:Key="ForegroundBrush" Color="#E0E0E0"/>
    <SolidColorBrush x:Key="DisabledForegroundBrush" Color="#656565"/>

    <SolidColorBrush x:Key="BorderBrush" Color="Transparent"/>
    <SolidColorBrush x:Key="DisabledBorderBrush" Color="#404040"/>
    <SolidColorBrush x:Key="ClickableBorderBrush" Color="#707070"/>
    <SolidColorBrush x:Key="HoverBorderBrush" Color="#3C7FB1"/>

    <SolidColorBrush x:Key="ComboBoxItemBackgroundBrush" Color="#3A3A3A"/>
    <SolidColorBrush x:Key="DataGridHeaderBorderBrush" Color="#606060"/>
    <SolidColorBrush x:Key="ScrollBarBackgroundBrush" Color="#484848"/>

    <SolidColorBrush x:Key="ErrorBrush" Color="#7F1818"/>

    <Thickness x:Key="GroupBoxBorderThickness">0.1</Thickness>

    <SolidColorBrush x:Key="CoreUsageBarBackgroundBrush" Color="#1E1E1E"/>
    <SolidColorBrush x:Key="CoreUsageBarParkedBackgroundBrush" Color="#505050"/>
    <SolidColorBrush x:Key="CoreUsageBarForegroundBrush" Color="#00A9D3"/>
    <SolidColorBrush x:Key="CoreUsageBarParkedForegroundBrush" Color="#6A6A6A"/>
    <SolidColorBrush x:Key="CoreUsageBarBorderBrush" Color="#505050"/>

</ResourceDictionary>
